      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "import \"experimental\"\n\n// Living room occupancy hours per day\noccupancy = from(bucket: \"home-assistant\")\n  |> range(start: -90d)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"binary_sensor.presence_livingroom\")\n  |> filter(fn: (r) => r[\"_field\"] == \"state\")\n  |> stateDuration(fn: (r) => r._value == \"on\", unit: 1h)\n  |> aggregateWindow(every: 1d, fn: sum, createEmpty: false)\n  |> set(key: \"_field\", value: \"Living Room Hours\")\n  |> yield(name: \"occupancy\")",
          "refId": "A"
        },
        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "import \"experimental\"\nimport \"date\"\n\n// Sun above horizon duration (daylight hours)\nfrom(bucket: \"home-assistant\")\n  |> range(start: -90d)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"sun.sun\")\n  |> filter(fn: (r) => r[\"_field\"] == \"state\")\n  |> stateDuration(fn: (r) => r._value == \"above_horizon\", unit: 1h)\n  |> aggregateWindow(every: 1d, fn: sum, createEmpty: false)\n  |> set(key: \"_field\", value: \"Daylight Hours\")\n  |> yield(name: \"daylight\")",
          "refId": "B"
        }
