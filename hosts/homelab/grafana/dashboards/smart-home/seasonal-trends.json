        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "// Month-over-month occupancy pattern comparison\nfrom(bucket: \"home-assistant\")\n  |> range(start: -12mo)\n  |> filter(fn: (r) => r[\"domain\"] == \"binary_sensor\")\n  |> filter(fn: (r) => r[\"entity_id\"] =~ /presence/)\n  |> filter(fn: (r) => r[\"_field\"] == \"state\")\n  |> stateDuration(fn: (r) => r._value == \"on\", unit: 1h)\n  |> aggregateWindow(every: 1mo, fn: sum, createEmpty: false)\n  |> group(columns: [\"entity_id\"])\n  |> yield(name: \"monthly_occupancy\")",
          "refId": "A"
        }
      ],
      "title": "Monthly Occupancy Trends (12 Months)",
      "type": "timeseries"
