        {
          "datasource": {
            "type": "influxdb",
            "uid": "${DS_INFLUXDB}"
          },
          "query": "// Temperature vs occupancy scatter plot\nimport \"join\"\n\n// Daily average outdoor temperature from Met weather integration\ntemp = from(bucket: \"home-assistant\")\n  |> range(start: -90d)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"weather.home\")\n  |> filter(fn: (r) => r[\"_field\"] == \"temperature\")\n  |> aggregateWindow(every: 1d, fn: mean, createEmpty: false)\n  |> set(key: \"metric\", value: \"temperature\")\n\n// Daily occupancy hours\noccupancy = from(bucket: \"home-assistant\")\n  |> range(start: -90d)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"binary_sensor.presence_livingroom\")\n  |> filter(fn: (r) => r[\"_field\"] == \"state\")\n  |> stateDuration(fn: (r) => r._value == \"on\", unit: 1h)\n  |> aggregateWindow(every: 1d, fn: sum, createEmpty: false)\n  |> set(key: \"metric\", value: \"occupancy\")\n\n// Join and display\njoin.time(\n    left: temp,\n    right: occupancy,\n    as: (l, r) => ({l with occupancy: r._value})\n  )\n  |> keep(columns: [\"_time\", \"_value\", \"occupancy\"])\n  |> yield(name: \"correlation\")",
          "refId": "A"
        }
      ],
