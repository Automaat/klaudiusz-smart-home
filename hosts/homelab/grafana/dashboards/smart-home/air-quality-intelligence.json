          },
          "query": "from(bucket: \"home-assistant\")\n  |> range(start: -30d)\n  |> filter(fn: (r) => r[\"entity_id\"] == \"sensor.airly_home_pm2_5\")\n  |> filter(fn: (r) => r[\"_field\"] == \"value\")\n  |> aggregateWindow(every: 1h, fn: mean, createEmpty: false)\n  |> map(fn: (r) => ({r with outdoor: r._value}))\n  |> keep(columns: [\"_time\", \"outdoor\"])\n  |> join(\n    tables: {\n      indoor: from(bucket: \"home-assistant\")\n        |> range(start: -30d)\n        |> filter(fn: (r) => r[\"entity_id\"] == \"sensor.zhimi_de_334622045_mb3_pm2_5_density_p_3_6\")\n        |> filter(fn: (r) => r[\"_field\"] == \"value\")\n        |> aggregateWindow(every: 1h, fn: mean, createEmpty: false)\n        |> map(fn: (r) => ({r with indoor: r._value}))\n        |> keep(columns: [\"_time\", \"indoor\"])\n    },\n    on: [\"_time\"]\n  )\n  |> map(fn: (r) => ({\n    _time: r._time,\n    _value: if r.outdoor > 0.0 then ((r.outdoor - r.indoor) / r.outdoor) * 100.0 else 0.0\n  }))\n  |> mean()",
          "refId": "A"
        }
      ],
